@{
}
<h2>Show Data</h2>
<table>
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Amount</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Title</td>
            <td>@item.Amount</td>
        </tr>
    }
</table>
